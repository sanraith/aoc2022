            "request": "launch",
            "name": "Debug unit tests in library 'aoc-cli'",
            "cargo": {
                "args": [
                    "test",
                    "--no-run",
                    "--lib",
                    "--package=aoc-cli"
                ],
                "filter": {
                    "name": "aoc-cli",
                    "kind": "lib"
                }
            },
            "args": [],
            "cwd": "${workspaceFolder}"
        },
        {
            "type": "lldb",
            "request": "launch",
            "name": "Debug executable 'aoc-cli'",
            "cargo": {
                "args": [
                    "build",
                    "--bin=aoc-cli",
                    "--package=aoc-cli"
                ],
                "filter": {
                    "name": "aoc-cli",
                    "kind": "bin"
                }
            },
            "args": [
                "solve",
                "19"
            ],
            "cwd": "${workspaceFolder}"
        },
